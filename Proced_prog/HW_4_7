#define _USE_MATH_DEFINES

#include <iostream>
#include <math.h>

using namespace std;

double nedorand(int m, int b, int c, int rand, int i)
{
    if (i == 0)
    {
        return rand;
    }
    else
    {
        rand = (m * rand + b) % c;
        //cout << rand << endl;
        return nedorand(m, b, c, rand, i - 1);
    }
}

int main()
{
    setlocale(LC_ALL, "Russian");
    int m, b, c;
    cin >> m ;
    cin >> b;
    cin >> c;
    cout << nedorand(m, b, c, 0, 999) << endl;
    return 0;
}
