#define _USE_MATH_DEFINES

#include <iostream>
#include <math.h>

using namespace std;


int main()
{
    setlocale(LC_ALL, "Russian");
    double x,y,b;

    cout << "Введите x" << endl;
    cin >> x;
    cout << "Введите y" << endl;
    cin >> y;
    cout << "Введите b" << endl;
    cin >> b;
    
    if (b - y <= 0 || b < x)
    {
        cout << "Ошибка" << endl;
    }
    else
    {
        cout << log(b - y) * (sqrt(b - x));
    }
    
	return 0;
}
