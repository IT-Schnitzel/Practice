#define _USE_MATH_DEFINES

#include <iostream>
#include <fstream>
// #include <math.h>

using namespace std;

int main()
{
    setlocale(LC_ALL, "Russian");
    for (int i = 0; i < 26; i++)
    {
        if (i < 10)
        {
            if (i % 2 == 0)
            {
                
                cout << "* * * * * * ";
                cout << "//////////////////////////////////////////////////////////////" << endl;
            }
            else
            {
                cout << " * * * * *" << endl;
            }
        }
        else
        {
            if (i % 2 == 0)
            {
                cout << "//////////////////////////////////////////////////////////////////////////" << endl;
            }
            else
            {
                cout << " " << endl;
            }

        }
    }
}
