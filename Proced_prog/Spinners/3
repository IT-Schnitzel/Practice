#include "iostream"

using namespace std;



int main()
{
  setlocale(LC_ALL, "Rus");
  int a, b;
  cin >> a >> b;
  long long summ = 0;
  for (int i = 1; i <= a; i++)
  {
    for (int j = 1; j <= b; j++)
    {
      summ = summ + a*b - (j-1)*a - (i-1)*b + (i-1)*(j-1);
      //cout << a * b - (j - 1) * a - (i - 1) * b + (i - 1) * (j - 1) << endl;
      //cout << i << " " << j << endl;

    }
  }
  cout << summ << endl;
}
